{
  "name": "unusuals-public-website",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Soluble Studio <tech@solublestudio.com>",
  "homepage": "https://solublestudio.com/",
  "dependencies": {
    "@solublestudio/gatsby-plugin-cookiehub": "^1.0.0",
    "@solublestudio/gatsby-theme-soluble-source": "1.4.7",
    "@solublestudio/unusuals-design-system": "0.0.33",
    "dotenv": "^8.2.0",
    "gatsby": "^2.32.8",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-google-tagmanager": "2.11.0",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-scroll-reveal": "^0.0.7",
    "gatsby-plugin-sitemap": "^2.12.0",
    "react": "16.14.0",
    "react-dom": "16.14.0"
  },
  "devDependencies": {
    "@netlify/plugin-gatsby": "^1.0.3",
    "prettier": "^2.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby develop -H 0.0.0.0 -p 1234",
    "format": "prettier --write \"**/*.{js,jsx,json,md,mdx}\"",
    "start": "npm run clean && gatsby develop -H 0.0.0.0 -p 1234",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/solublestudio/gatsby-starter-soluble"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
